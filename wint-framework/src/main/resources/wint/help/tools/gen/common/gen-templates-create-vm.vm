${formCreateDefine}
<h1>create ${alias}</h1>

<form action="$doCreateAction" method="post">
    $securityUtil.tokenHtml
    #foreach($field in $fields)
    <div>
        <label>
            ${field.name}
            <input type="text" name="$form.${field.name}.name" value="$field.fieldValue" />
        </label>
        $field.fieldMessage
    </div>
    #end

    <div>
        <input type="submit" value="create!" />
    </div>

</form>
